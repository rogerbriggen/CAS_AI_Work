{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    { "name": "Python Debugger: Current File", "type": "debugpy", "request": "launch", "program": "${file}", "console": "integratedTerminal" },
    {
      "name": "Python Debugger: Current File with Arguments",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": ["${command:pickArgs}"]
    },
    {
      "name": "Python Debugger: Current File with -pt=extractText",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": ["-pt=extractText"]
    },
    {
      "name": "Python Debugger: Current File with -pt=createEmbeddings",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": ["-pt=createEmbeddings"]
    },
    {
      "name": "Python Debugger: Current File with -pt=runRag",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": ["-pt=runRag"]
    },
    {
      "name": "Python Debugger: Current File with -pt=processAll",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": ["-pt=processAll"]
    },
    {
      "name": "Python Debugger: Current File with -pt=runRag Sec En",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "-pt=runRag",
        "--pdfPath=data/example_pdfs_securiton_en/",
        "--jsonFile=temp/pdf_demo_sec_en/parsed_pdf_docs.json",
        "--embeddingsFile=temp/pdf_demo_sec_en/parsed_pdf_docs_with_embeddings.csv"
      ]
    },
    {
      "name": "Python Debugger: Current File with -pt=processAll Sec En",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "-pt=processAll",
        "--pdfPath=data/example_pdfs_securiton_en/",
        "--jsonFile=temp/pdf_demo_sec_en/parsed_pdf_docs.json",
        "--embeddingsFile=temp/pdf_demo_sec_en/parsed_pdf_docs_with_embeddings.csv"
      ]
    }
  ]
}
